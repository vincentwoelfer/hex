config_version=5

[application]

config/name="hex"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
run/max_fps=500
boot_splash/show_image=false
boot_splash/image="res://assets/textures/splash.png"
config/icon="res://assets/ui/icons/gameart_icon.png"

[autoload]

GameStateManager="*res://src/autoload/GameStateManager.gd"
EventBus="*res://src/autoload/EventBus.gd"
ResLoader="*res://src/autoload/ResLoader.gd"
MapGeneration="*res://src/autoload/MapGeneration.gd"
PlayerManager="*res://src/autoload/PlayerManager.gd"
HexInput="*res://src/autoload/HexInput.gd"
